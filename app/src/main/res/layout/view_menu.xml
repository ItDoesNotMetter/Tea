<?xml version="1.0" encoding="utf-8"?>
<com.fengjie.percentlibrary.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayout_menu"
    style="@style/w_0_h_0"
    app:layout_heightPercent="15%w"
    app:layout_widthPercent="100%w"
    android:layout_alignParentTop="true">

        <ImageButton
            android:id="@+id/back_button_menu"
            style="@style/menu_button_back"/>

        <TextView
            android:id="@+id/title_textView_menu"
                style="@style/w_w_h_w"
            android:textColor="@color/white"
            android:text="Title"
            android:gravity="center"
            android:textSize="25sp"
            android:layout_centerInParent="true" />

        <ImageButton
            android:id="@+id/operate_imageButton_menu"
            style="@style/menu_button_operate"/>


</com.fengjie.percentlibrary.PercentRelativeLayout>